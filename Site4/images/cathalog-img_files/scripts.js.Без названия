$(document).ready(function(){
   $.validator.addMethod("defaultInvalid", function(value, element) 
     {
      return !(element.value == element.defaultValue);
     });
    function fixEvent(e) {
        e = e || window.event;
        if(!e.target) e.target = e.srcElement;
        return e;
    }

    function closePopup(e, popup) {
        var container = $(popup);
        if (container.has(e.target).length === 0){
             container.hide();
        }
    }


    function thanks() {
        $(".thank-window").fadeIn();
        setTimeout(function(){
            $('.thank-window .close-block').trigger("click");
        }, 3000);
    }

    $("#back-top").hide();
  
      // fade in #back-top
      $(function () {
        $(window).scroll(function () {
          if ($(this).scrollTop() > 600) {
            $('#back-top').fadeIn();
          } else {
            $('#back-top').fadeOut();
          }
        });

        // scroll body to 0px on click
        $('#back-top a').click(function () {
          $('body,html').animate({
            scrollTop: 0
          }, 1500);
          return false;
        });
      });

      $(".msk").click(function(){
      $(".msk").hide();
        $(".mskk").fadeIn();
          $(".spb").fadeIn();
            $(".last-li").mouseleave(function(){
              $(".mskk").hide();
                $(".spb").hide();
                  $(".msk").show();
              });
        return false;
    });

    

    $('.thank-window .close-block').click(function() {
    $('.thank-window').fadeOut();
    });

    $('html').click(function(){
        $('.thank-window').fadeOut();
    })
    
    $('.thank-window .block').click(function(event){
        return false;
    });

    $(".btn-order").click(function(){
        $(".lightbox1").fadeIn();
    });

    $(".btn-order2").click(function(){
        $(".lightbox3").fadeIn();
    });

    $(".btn-order3").click(function(){
        $(".lightbox4").fadeIn();
    });

    $(".btn-download").click(function(){
        $(".lightbox5").fadeIn();
    });

    $(".btn-orderr, .btn-wanna").click(function(){
        $(".lightbox6").fadeIn();
    });

    $(".btn-call-back").click(function(){
        $(".lightbox7").fadeIn();
    });

    $(".btn-sales").click(function(){
        $(".lightbox8").fadeIn();
    });


    $(".lightbox-close").click(function(){
        $(".lightbox1,.lightbox2,.lightbox3,.lightbox4,.lightbox5,.lightbox6,.lightbox7,.lightbox8").fadeOut();
    });
   
    $(document).mouseup(function (e) {
        e = fixEvent(e);
        closePopup(e, ".lightbox1,.lightbox2,.lightbox3,.lightbox4,.lightbox5,.lightbox6,.lightbox7,.lightbox8");
    });
    
    $("#phone222").mask("+7 (999) 999-9999");

    $('.phone').groupinputs();

    $(".form1").validate({
        rules: {
            name:{
                required: true,
                defaultInvalid: true,
                minlength:1
            }, 
            phone1: {
                required:true,
                defaultInvalid: true,
                minlength: 1
            },
            phone2: {
                required:true,
                defaultInvalid: true,
                minlength: 3
            },
            phone3: {
                required:true,
                defaultInvalid: true,
                minlength: 7
            },
           
        },
        submitHandler: function(form) {
            $.ajax({
               type: "POST",
               url: $(form).attr('action'),
               data: $(form).serialize(), // serializes the form's elements.
               success: function(data) {
                   if(data == '–°–ø–∞—Å–∏–±–æ –∑–∞ –∑–∞—è–≤–∫—É!') {
                        $(".lightbox-close").trigger("click");
                        form.reset();
                        thanks();
                    }
                }
            });
            return false;
        }
    });

