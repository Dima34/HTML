    $(document).ready(function(){
function recountCalc(){
   

    s = parseInt($('#inpS').val().replace('_', ''));
    a = parseInt($('#inpA').val().replace('_', ''));
    if ($('#inpL').val()==""){
        l = 0;
    }
    else{
    l = parseInt($('#inpL').val().replace('_', ''));
    }
    s1 = 0;
    a1 = 0;
    l1 = 0;

    if (s <10 ){
        s1 = s * 550;
    }

    if (s >= 10 && s < 20 ){
        s1 = s * 550;
    }

    if (s >= 20 ){
        s1 = s * 550;
    }

    if (a > 4){
        a1 = (a-4) * 200;
    }

    l1 = l * 600;

    price = l1 + a1 + s1;

    $('.cal_total h3').text(price);
    $('#inpR').val(price);

}

    $('.cal_var input').change(function(){
        recountCalc();
    });
    $('.cal_var input').keyup(function(){
        recountCalc();
    });

    $('#inpL').focusout(function(){
         if ($('#inpL').val()==""){
       $('#inpL').val('0');
       recountCalc();
       return false; 
    }
    });
     recountCalc();
});

